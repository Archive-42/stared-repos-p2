<h2>Log In</h2>

<form action="<%= session_url %>" method="post">
    <%= auth_token %>
    <label>
        Username
        <input type="text" name="user[username]">
    </label>
    <label>
        Password
        <input type="text" name="user[password]">
    </label>
    <button>Log In</button>
</form>